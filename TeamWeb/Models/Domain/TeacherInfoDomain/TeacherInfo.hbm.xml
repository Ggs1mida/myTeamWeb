<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TeamWeb" namespace="TeamWeb.Models.Domain.TeacherInfoDomain" default-lazy="false">
  <class name="TeacherInfo" table="TEACHERINFO">
    
    <id name="Id">
      <column name="ID" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="UserCode" column=" USERCODE"/>
    <property name="Name" column="NAME"/>

    <property name="Title" column="TITLE"/>
    <property name="Credit" column="CREDIT"/>
    <property name="Direction" column="DIRECTION"/>

    <property name="Introduce" column="INTRODUCE"/>

    <property name="Photo" column="PHOTO"/>
  </class>
</hibernate-mapping>
