<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TeamWeb" namespace="TeamWeb.Models.Domain.WorkDomain" default-lazy="false">
  <class name="WorkInfo" table="WORKINFO">
    
    <id name="Id">
      <column name="ID" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="UserCode" column=" USERCODE"/>
    <property name="Name" column="NAME"/>

    <property name="Company" column="COMPANY"/>
  </class>
</hibernate-mapping>
