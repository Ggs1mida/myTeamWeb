<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TeamWeb" namespace="TeamWeb.Models.Domain.NewsInfoDomain" default-lazy="false">
  <class name="NewsInfo" table="NewsInfo">
    
    <id name="Id">
      <column name="ID" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="Title" column=" TITLE"/>
    <property name="Time" column="TIME"/>

    <property name="Introduce" column="INTRODUCE"/>
  <property name="Name" column="NAME"/>
</class>
</hibernate-mapping>
