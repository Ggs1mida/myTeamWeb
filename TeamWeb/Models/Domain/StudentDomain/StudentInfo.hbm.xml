<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TeamWeb" namespace="TeamWeb.Models.Domain.StudentDomain" default-lazy="false">
  <class name="StudentInfo" table="STUDENTINFO">
    <id name="ID">
      <column name="ID" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>

    <property name="USERCODE" column="USERCODE"/>

    <property name="INTRODUCE" column="INTRODUCE"/>

    <property name="SCHOOLSTART" column="SCHOOLSTART"/>

    <property name="END" column="END"/>

    <property name="MAJOR" column="MAJOR"/>
    
    <property name="PHOTO" column="PHOTO"/>

    <property name="NAME" column="NAME"/>
    
    <property name="DEGREE" column="DEGREE"/>
    
  <property name="GRADUATE" column="GRADUATE"/>

  </class>

 

</hibernate-mapping>
