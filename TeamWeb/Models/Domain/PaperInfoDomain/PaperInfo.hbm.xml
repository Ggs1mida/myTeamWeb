<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TeamWeb" namespace="TeamWeb.Models.Domain.PaperInfoDomain" default-lazy="false">
  <class name="PaperInfo" table="PAPERINFO">
    <id name="ID">
      <column name="ID" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="PAPERNAME" column="PAPERNAME"/>
    <property name="PAPERDESCRIBE" column="PAPERDESCRIBE"/>
    <property name="PAPERAUTHOR" column="PAPERAUTHOR"/>
    <property name="PAPERYEAR" column="PAPERYEAR"/>
    <property name="PAPERADDRESS" column="PAPERADDRESS"/>
    <property name="PAPERTYPE" column="PAPERTYPE"/>
    <property name="UPLOADTIME" column="UPLOADTIME"/>
  </class>
</hibernate-mapping>


